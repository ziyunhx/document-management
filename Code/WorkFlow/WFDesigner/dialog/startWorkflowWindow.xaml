<Window x:Class="WFDesigner.dialog.startWorkflowWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="startWorkflowWindow" Height="400" Width="400"
        WindowStartupLocation="CenterScreen" WindowStyle="ToolWindow"
        >
    <StackPanel Height="370" Width="400">
        <StackPanel Height="330" Width="400" Name="body"></StackPanel>
        <StackPanel Orientation="Horizontal">
            <Button Width="100" Height="25" Content="参数启动" Name="argumentStartButton" Margin="5,5,5,5" Click="Button_Click"></Button>
            <Button Width="100" Height="25" Content="无参数启动" Margin="5,5,5,5" Click="Button_Click"></Button>
            <Button Width="100" Height="25" Content="取消" Margin="5,5,5,5" Click="Button_Click"></Button>
        </StackPanel>
    </StackPanel>
</Window>
