<Window x:Class="WFDesigner.dialog.openChildWorkflowWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="openChildWorkflowWindow" Height="300" Width="374">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="35" />
        </Grid.RowDefinitions>
        <ContentControl HorizontalAlignment="Stretch"  VerticalAlignment="Stretch"    Name="desienerPanel"  />
        <Button Content="浏览" Grid.Row="1" Height="23" HorizontalAlignment="Right"   Name="browserButton" VerticalAlignment="Top" Width="75" Margin="0,5,220,0" Click="browserButton_Click" />
        <Button Content="插入" Grid.Row="1" Height="23" HorizontalAlignment="Right"   Name="insertButton" VerticalAlignment="Top" Width="75" Margin="0,5,110,0" Click="insertButton_Click" />
        <Button Content="取消" Grid.Row="1" Height="23" HorizontalAlignment="Right"   Name="cancelButton" VerticalAlignment="Top" Width="75" Margin="0,5,10,0" Click="cancelButton_Click" />
    </Grid>
</Window>
